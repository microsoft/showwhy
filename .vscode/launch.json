    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Remote Attach - Exposure API",
        "type": "python",
        "request": "attach",
        "connect": {
          "host": "localhost",
          "port": 6900
        },
        "pathMappings": [
          {
            "localRoot": "${workspaceFolder}/python/backend",
            "remoteRoot": "/backend"
          }
        ]
      },
      {
        "name": "Python: Remote Attach - Exposure Worker",
        "type": "python",
        "request": "attach",
        "connect": {
          "host": "localhost",
          "port": 6901
        },
        "pathMappings": [
          {
            "localRoot": "${workspaceFolder}/python/backend",
            "remoteRoot": "/backend"
          }
        ]
      }
    ]
  }
  