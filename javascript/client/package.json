	"scripts": {
		"clean": "essex clean build",
		"bundle": "vite build",
		"build": "yarn typecheck && yarn bundle",
		"typecheck": "tsc -b .",
		"build-dev": "yarn typecheck && yarn bundle --mode dev",
		"build:client-dev": "yarn build-dev && yarn archive",
		"build:only": "yarn build",
		"build:client": "yarn build && yarn archive",
		"start": "vite",
		"start:client": "yarn start",
		"start:static": "http-server -p 3000 dist",
		"archive": "essex zip --baseDir dist deploy.zip '**/*'",
		"test": "NODE_OPTIONS=--experimental-vm-modules yarn node $(yarn bin jest) --no-cache",
		"test:jest": "yarn test",
		"test:cov": "jest --coverage"
	},
